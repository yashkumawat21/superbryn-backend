{
  "name": "superbryn-backend",
  "version": "1.0.0",
  "description": "AI Voice Agent Backend with LiveKit",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "agent": "tsx src/agent/livekit-agent.ts dev",
    "agent:build": "node dist/agent/livekit-agent.js dev",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "livekit",
    "ai",
    "voice-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cartesia/cartesia-js": "^2.2.9",
    "@deepgram/sdk": "^3.6.0",
    "@livekit/agents": "1.0.37",
    "@livekit/agents-plugin-cartesia": "1.0.37",
    "@livekit/agents-plugin-deepgram": "1.0.37",
    "@livekit/agents-plugin-openai": "1.0.37",
    "@livekit/agents-plugin-silero": "1.0.37",
    "@livekit/protocol": "^1.10.0",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "livekit-server-sdk": "^2.4.0",
    "openai": "^4.47.1",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
